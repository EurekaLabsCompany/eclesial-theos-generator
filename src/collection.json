{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {    
    "application": {
      "description": "A Theos application.",
      "factory": "./application/index#application",
      "schema": "./application/schema.json"
    },
    "component": {
      "description": "A Theos component.",
      "factory": "./component/index#component",
      "schema": "./component/schema.json"
    },
    "search": {
      "description": "A Theos search.",
      "factory": "./search/index#search",
      "schema": "./search/schema.json"
    },
    "search-input": {
      "description": "A Theos Search Input.",
      "factory": "./search-input/index#searchInput",
      "schema": "./search-input/schema.json"
    },
    "service": {
      "description": "A Theos Service.",
      "factory": "./service/index#service",
      "schema": "./service/schema.json"
    },
    "viewmodel": {
      "description": "A Theos ViewModels.",
      "factory": "./viewmodel/index#viewmodel",
      "schema": "./viewmodel/schema.json"
    },
    "severino" : {
      "description": "All generators in one place.",
      "factory": "./severino/index#severino",
      "schema": "./severino/schema.json"
    }
  }
}
